                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/redirectionClass",
                    "type": "string",
                    "title": "The Redirectionclass Schema ",
                    "default": "",
                    "examples": [
                      "org.cofax.SqlRedirection"
