        "servlet-mapping": {
          "$id": "/properties/web-app/properties/servlet-mapping",
          "type": "object",
          "properties": {
            "cofaxCDS": {
              "$id": "/properties/web-app/properties/servlet-mapping/properties/cofaxCDS",
              "type": "string",
              "title": "The Cofaxcds Schema ",
              "default": "",
              "examples": [
                "/"
              ]
            },
            "cofaxEmail": {
              "$id": "/properties/web-app/properties/servlet-mapping/properties/cofaxEmail",
              "type": "string",
              "title": "The Cofaxemail Schema ",
              "default": "",
              "examples": [
                "/cofaxutil/aemail/*"
              ]
            },
            "cofaxAdmin": {
              "$id": "/properties/web-app/properties/servlet-mapping/properties/cofaxAdmin",
              "type": "string",
              "title": "The Cofaxadmin Schema ",
              "default": "",
              "examples": [
                "/admin/*"
              ]
            },
            "fileServlet": {
              "$id": "/properties/web-app/properties/servlet-mapping/properties/fileServlet",
              "type": "string",
              "title": "The Fileservlet Schema ",
              "default": "",
              "examples": [
                "/static/*"
              ]
            },
            "cofaxTools": {
              "$id": "/properties/web-app/properties/servlet-mapping/properties/cofaxTools",
              "type": "string",
              "title": "The Cofaxtools Schema ",
              "default": "",
              "examples": [
                "/tools/*"
              ]
            }
          }
        },
        "taglib": {
          "$id": "/properties/web-app/properties/taglib",
          "type": "object",
          "properties": {
            "taglib-uri": {
              "$id": "/properties/web-app/properties/taglib/properties/taglib-uri",
              "type": "string",
              "title": "The Taglib-uri Schema ",
              "default": "",
              "examples": [
                "cofax.tld"
              ]
            },
            "taglib-location": {
              "$id": "/properties/web-app/properties/taglib/properties/taglib-location",
              "type": "string",
              "title": "The Taglib-location Schema ",
              "default": "",
              "examples": [
                "/WEB-INF/tlds/cofax.tld"
              ]
            }
          }
        }
      }
    }
  }
}