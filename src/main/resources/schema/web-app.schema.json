                    "title": "The Cachepagestrack Schema ",
                    "default": 0,
                    "examples": [
                      200
                    ]
                  },
                  "cachePagesStore": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/cachePagesStore",
