                  "templateOverridePath": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/templateOverridePath",
                    "type": "string",
                    "title": "The Templateoverridepath Schema ",
                    "default": "",
                    "examples": [
                      ""
                    ]
                  },
                  "defaultListTemplate": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/defaultListTemplate",
                    "type": "string",
                    "title": "The Defaultlisttemplate Schema ",
                    "default": "",
                    "examples": [
                      "listTemplate.htm"
                    ]
                  },
                  "defaultFileTemplate": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/defaultFileTemplate",
                    "type": "string",
                    "title": "The Defaultfiletemplate Schema ",
