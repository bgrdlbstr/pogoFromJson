

{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "web-app": {
      "$id": "/properties/web-app",
      "type": "object",
      "properties": {
        "servlet": {
          "$id": "/properties/web-app/properties/servlet",
          "type": "array",
          "items": {
            "$id": "/properties/web-app/properties/servlet/items",
            "type": "object",
            "properties": {
              "servlet-name": {
                "$id": "/properties/web-app/properties/servlet/items/properties/servlet-name",
                "type": "string",
                "title": "The Servlet-name Schema ",
                "default": "",
                "examples": [
                  "cofaxCDS"
                ]
              },
              "servlet-class": {
                "$id": "/properties/web-app/properties/servlet/items/properties/servlet-class",
                "type": "string",
                "title": "The Servlet-class Schema ",
                "default": "",
                "examples": [
                  "org.cofax.cds.CDSServlet"
                ]
              },
              "init-param": {
                "$id": "/properties/web-app/properties/servlet/items/properties/init-param",
                "type": "object",
                "properties": {
                  "configGlossary:installationAt": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/configGlossary:installationAt",
                    "type": "string",
                    "title": "The Configglossary:installationat Schema ",
                    "default": "",
                    "examples": [
                      "Philadelphia, PA"
                    ]
                  },
                  "configGlossary:adminEmail": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/configGlossary:adminEmail",
                    "type": "string",
                    "title": "The Configglossary:adminemail Schema ",
                    "default": "",
                    "examples": [
                      "ksm@pobox.com"
                    ]
                  },
                  "configGlossary:poweredBy": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/configGlossary:poweredBy",
                    "type": "string",
                    "title": "The Configglossary:poweredby Schema ",
                    "default": "",
                    "examples": [
                      "Cofax"
                    ]
                  },
                  "configGlossary:poweredByIcon": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/configGlossary:poweredByIcon",
                    "type": "string",
                    "title": "The Configglossary:poweredbyicon Schema ",
                    "default": "",
                    "examples": [
                      "/images/cofax.gif"
                    ]
                  },
                  "configGlossary:staticPath": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/configGlossary:staticPath",
                    "type": "string",
                    "title": "The Configglossary:staticpath Schema ",
                    "default": "",
                    "examples": [
                      "/content/static"
                    ]
                  },
                  "templateProcessorClass": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/templateProcessorClass",
                    "type": "string",
                    "title": "The Templateprocessorclass Schema ",
                    "default": "",
                    "examples": [
                      "org.cofax.WysiwygTemplate"
                    ]
