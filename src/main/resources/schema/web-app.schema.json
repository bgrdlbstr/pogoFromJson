                  "jspListTemplate": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/jspListTemplate",
                    "type": "string",
                    "title": "The Jsplisttemplate Schema ",
                    "default": "",
                    "examples": [
                      "listTemplate.jsp"
                    ]
                  },
                  "jspFileTemplate": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/jspFileTemplate",
                    "type": "string",
                    "title": "The Jspfiletemplate Schema ",
                    "default": "",
                    "examples": [
                      "articleTemplate.jsp"
