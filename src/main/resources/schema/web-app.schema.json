                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreUrl",
                    "type": "string",
                    "title": "The Datastoreurl Schema ",
                    "default": "",
                    "examples": [
                      "jdbc:microsoft:sqlserver://LOCALHOST:1433;DatabaseName=goon"
                    ]
                  },
                  "dataStoreUser": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreUser",
                    "type": "string",
                    "title": "The Datastoreuser Schema ",
                    "default": "",
                    "examples": [
                      "sa"
                    ]
                  },
                  "dataStorePassword": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStorePassword",
                    "type": "string",
                    "title": "The Datastorepassword Schema ",
                    "default": "",
                    "examples": [
                      "dataStoreTestQuery"
                    ]
                  },
                  "dataStoreTestQuery": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreTestQuery",
                    "type": "string",
                    "title": "The Datastoretestquery Schema ",
                    "default": "",
                    "examples": [
                      "SET NOCOUNT ON;select test='test';"
                    ]
                  },
                  "dataStoreLogFile": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreLogFile",
                    "type": "string",
                    "title": "The Datastorelogfile Schema ",
                    "default": "",
                    "examples": [
                      "/usr/local/tomcat/logs/datastore.log"
                    ]
                  },
                  "dataStoreInitConns": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreInitConns",
                    "type": "integer",
                    "title": "The Datastoreinitconns Schema ",
                    "default": 0,
                    "examples": [
                      10
                    ]
                  },
                  "dataStoreMaxConns": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreMaxConns",
                    "type": "integer",
                    "title": "The Datastoremaxconns Schema ",
                    "default": 0,
                    "examples": [
                      100
                    ]
                  },
                  "dataStoreConnUsageLimit": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreConnUsageLimit",
                    "type": "integer",
                    "title": "The Datastoreconnusagelimit Schema ",
                    "default": 0,
                    "examples": [
                      100
                    ]
                  },
                  "dataStoreLogLevel": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/dataStoreLogLevel",
                    "type": "string",
                    "title": "The Datastoreloglevel Schema ",
                    "default": "",
                    "examples": [
                      "debug"
                    ]
                  },
                  "maxUrlLength": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/maxUrlLength",
                    "type": "integer",
                    "title": "The Maxurllength Schema ",
                    "default": 0,
                    "examples": [
                      500
                    ]
                  }
                }
