                    "title": "The Cachepackagetagsrefresh Schema ",
                    "default": 0,
                    "examples": [
                      60
                    ]
                  },
                  "cacheTemplatesTrack": {
                    "$id": "/properties/web-app/properties/servlet/items/properties/init-param/properties/cacheTemplatesTrack",
                    "type": "integer",
                    "title": "The Cachetemplatestrack Schema ",
                    "default": 0,
                    "examples": [
                      100
                    ]
